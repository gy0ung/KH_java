	public static void main(String[] args) {
		try {
			int port = 3005;
			String ip = InetAddress.getLocalHost().getHostAddress();
			System.out.println(ip);
			Scanner sc = new Scanner(System.in);
			
		} catch (UnknownHostException e) {
			e.printStackTrace();
		}
		

	}

}
