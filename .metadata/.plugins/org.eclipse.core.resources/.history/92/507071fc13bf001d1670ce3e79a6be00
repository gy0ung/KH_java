package test0309;

import java.util.Arrays;

public class MaxDivisor {
	// 약수를 구하는 클래스
	int num;

	public MaxDivisor(int num) {
		super();
		this.num = num;

	int[] arr = {};
	
	for(int a = 1; a==num; a++) {
		if(num%a == 0) {
		int i = 0;
		arr[i] = a;
		i++;
		}
	}
	System.out.println(Arrays.toString(arr));
	}
	
}
